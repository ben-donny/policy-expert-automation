<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Home Insurance" verbose="1" parallel="false">

    <test name="Home insurance enquiry form running on chrome browser">
        <parameter name="BROWSER" value="chrome"/>
        <parameter name="SITEURL" value="https://insurance.policyexpert.co.uk/home"/>
        <parameter name="firstName" value="doni"/>
        <parameter name="lastName" value="Jonah"/>
        <classes>
          <class name="uk.co.policyexpert.HomeEnquiryFormTest" />
        </classes>
    </test>

      <test name="Home insurance enquiry form running on firefox browser">
            <parameter name="BROWSER" value="firefox"/>
            <parameter name="SITEURL" value="https://insurance.policyexpert.co.uk/home"/>
            <parameter name="firstName" value="doni"/>
            <parameter name="lastName" value="Jonah"/>
            <classes>
                <class name="uk.co.policyexpert.HomeEnquiryFormTest" />
            </classes>
      </test>

    </suite>
